{
    "name": "@agentos/proto",
    "version": "0.0.1",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npm run generate && tsc",
        "generate": "npx grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=outputServices=grpc-js,esModuleInterop=true -I./proto ./proto/*.proto",
        "lint": "eslint ."
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.9.0"
    },
    "devDependencies": {
        "@agentos/typescript-config": "*",
        "grpc-tools": "^1.12.0",
        "ts-proto": "^1.165.0",
        "typescript": "^5.3.0"
    }
}